{
  "name": "nasty-plot",
  "version": "0.1.0",
  "private": true,
  "packageManager": "pnpm@10.29.2",
  "workspaces": [
    "packages/*",
    "apps/*"
  ],
  "scripts": {
    "dev": "concurrently -n next,mcp,proxy -c blue,green,magenta \"turbo dev\" \"pnpm run dev:mcp\" \"pnpm run dev:proxy\"",
    "build": "turbo build",
    "lint": "turbo lint",
    "test": "turbo test",
    "test:coverage": "turbo test -- --coverage",
    "seed": "pnpm --filter @nasty-plot/data-pipeline run seed",
    "dev:mcp": "pnpm --filter @nasty-plot/mcp-server run dev",
    "dev:proxy": "bash scripts/dev-proxy.sh",
    "dev:all": "concurrently -n next,mcp,proxy -c blue,green,magenta \"pnpm run dev\" \"pnpm run dev:mcp\" \"pnpm run dev:proxy\"",
    "db:generate": "prisma generate",
    "db:migrate": "prisma migrate dev",
    "db:push": "prisma db push"
  },
  "dependencies": {
    "@prisma/client": "^7.3.0",
    "prisma": "^7.3.0"
  },
  "devDependencies": {
    "concurrently": "^9.2.1",
    "turbo": "^2.0.0",
    "typescript": "^5"
  },
  "pnpm": {
    "onlyBuiltDependencies": [
      "@prisma/engines",
      "better-sqlite3",
      "esbuild",
      "prisma",
      "sharp",
      "unrs-resolver"
    ]
  }
}
